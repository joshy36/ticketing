{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "jsx": "react-native",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "types": [
      "nativewind/types"
    ]
  },
  "include": [
    "src",
    "*.ts",
    "index.tsx",
    "*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
